<drawer id="drawer">
    <linear orientation="vertical">
        <!-- 应用头 -->
        <vertical w="*" h="100">
            <card h="90">
                <vertical>
                    <horizontal w="*" gravity="top">
                        <img id="logo" src="data:image/jpg;base64," w="60"
                            h="50sp"  padding="5sp"/>
                        <text id="titleBeg" text="掘金" textSize="25sp" textStyle="bold" textColor="#FFD700"
                            marginTop="10" layout_gravity="top" h="auto"/>
                        <text id="titleEnd" text="时代" textSize="25SP" textStyle="bold" textColor="#00FA9A"
                            marginTop="10" layout_gravity="top" h="auto" w="85"/>
                        <text text="掘    金    时   代    让    您    的    时    间    更    有    价    值  !" textStyle="bold" textSize="15sp" padding="10sp" gravity="left" textColor="#FFB90F" w="auto" layout_gravity="bottom"/>
                            <!-- <button id="unBindBtn" text="解绑卡密"
                            style="Widget.AppCompat.Button.Borderless.Colored" layout_gravity="center_vertical"/> -->
                        </horizontal>

                        <horizontal w="*" gravity="left">
                        <text text="本软件目前仅支持安卓系统7.0及以上系统使用." textStyle="italic" textSize="12sp" marginLeft="50" textColor="#CDC5BF" gravity="left"/>
                        <text text=" " w="225" />
                        <button id="logBtn" text="查看日志"
                            style="Widget.AppCompat.Button.Borderless.Colored" layout_gravity="right"/>
                        </horizontal>
                                                                       
                        </vertical>
                    </card>
                </vertical>
                <!-- 应用体 -->
                <vertical w="*" layout_weight="5">
                    <!-- 可上下滚动 -->
                    <viewpager id="viewpager">
                        <vertical>
                            <!-- 软件公告 -->
                            <card layout_widget="2">
                                <vertical padding="10" w="*">
                                    <text text="公告" textSize="16sp" textStyle="bold" layout_gravity="center" w="auto"/>
                                    <text id="announcementText" textColor="blue" layout_gravity="center" w="auto"/>
                                </vertical>
                            </card>
                            <button h="5" w="*"/>
                                <!-- 激活码 -->
                            <card layout_widget="1">
                                <vertical padding="10">
                                    <horizontal>
                                        <text text="激活码: " textStyle="bold" textSize="16sp"/>
                                        <input tag="EditText" id="activateCode" w="*" inputType="textVisiblePassword"/>
                                    </horizontal>
                                    <horizontal alpha="1">
                                        <text text="剩余时间: " textSize="16sp" textStyle="bold" textColor="#EE2C2C"
                                            layout_gravity="left|center_vertical" w="auto"/>
                                        <text text="-1 天" id="remainDayText" textSize="16sp" textStyle="bold"
                                            textColor="#EE00EE" layout_gravity="left|center_vertical" w="420"/>
                                        <button id="activateBtn" style="Widget.AppCompat.Button.Colored" text="登录验证"
                                            textSize="16sp" textStyle="bold" w="auto"/>

                                    </horizontal>

                                    <text text="激活码一旦激活无法在其他设备上二次激活，请谨慎激活！" textSize="12sp" textColor="#CDC5BF" w="auto"/>
                                </vertical>
                            </card>
                            <button h="5" w="*"/>
                                <!-- 基础设置 -->
                            <card layout_widget="2">
                                <vertical padding="10">
                                    <!-- 各种权限 -->
                                    <horizontal w="auto" h="auto">
                                        <vertical w="128">
                                            <Switch tag="Switch" id="swAccessibility" text="无障碍" textSize="16sp" h="35" 
                                                textStyle="bold" checked="{{auto.service!=null}}"/>
                                            <Switch tag="Switch" id="swFloatWindow" text="悬浮窗" textSize="16sp" h="35" 
                                                textStyle="bold" checked="true"/>     
                                            <Switch tag="Switch" id="swRoot" text="Root" textSize="16sp" h="35"
                                                textStyle="bold" checked="true"/>
                                        </vertical>
                                        <vertical layout_gravity="right" marginLeft="20" w="128">
                                            <Switch tag="Switch" id="swFlyModeBtn" text="换IP" textSize="16sp" h="35"
                                                textStyle="bold"/>
                                            <Switch tag="Switch" id="swCleanApp" text="清理后台" textSize="16sp" textStyle="bold" checked="true"  h="35"/>
                                            <Switch tag="Swtich" id="swSignIn" text="开启签到" textSize="16sp" textStyle="bold" checked="true"  h="35"/>
                                        </vertical>
                                        <vertical layout_gravity="right" marginLeft="20" w="128">
                                            <Switch text="虚拟定位" textSize="16sp" textStyle="bold" checked="true" h="35"/>
                                            <Switch text="滑块验证" textSize="16sp" textStyle="bold" checked="true" h="35"/>
                                            <Switch text="随机关注" textSize="16sp" textStyle="bold" checked="true" h="35" />

                                        </vertical>
                                        <vertical layout_gravity="right" marginLeft="20" w="128">
                                            <Switch tag="Switch" id="swLike" text="随机点赞" textSize="16sp" textStyle="bold" checked="true" h="35" />
                                            <Switch tag="Switch" id="swTaskCycle" text="任务循环" textSize="16sp" textStyle="bold" checked="true" h="35" />
                                            <Switch text="自动优化" textSize="16sp" textStyle="bold" checked="true" h="35" />
                                        </vertical>
                                    </horizontal>
                                    <!-- 执行完任务自动关机 -->
                                    <horizontal>
                                        <Switch tag="Switch" id="swPowerOff" text="执行完所有任务之后自动关机(ps:不能与任务循环一起使用)" textStyle="bold" h="35" w="*"/>
                                        <!-- <text text="执行完所有任务之后自动关机(ps:不能与任务循环一起使用)" textColor="#000000" textStyle="bold" textSize="16sp"/> -->
                                    </horizontal>
                                </vertical>
                            </card>
                            <button h="5" w="*"/>
                            <card layout_widget="3">
                                <viewpager id="appTypeViewPager">
                                    <!-- 视频播放类 -->
                                    <!-- 添加UI模板 -->
                                    <!-- 某某视频
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swXxxXxx"/>
                                                        <text text="某某视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="XxxXxxTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>
                                                       <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="XxxXxxSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="XxxXxxSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal> -->
                                    <scroll>
                                        <vertical>
                                            <text text="视频播放类" textSize="18p" textStyle="bold" layout_gravity="center_horizontal"
                                                w="auto"/>
                                                <!-- <list id="videoList" w="*"> -->
                                                    <!-- 快手 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swKuaiShou"/>
                                                        <text text="快手视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="kuaiShouTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>

                                                        <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="kuaiShouSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="kuaiShouswitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- 微视 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swWeiShi"/>
                                                        <text text="微视视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="weiShiTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>

                                                        <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="weiShiSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="weiShiSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- 刷宝视频 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swShuaBao"/>
                                                        <text text="刷宝视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="shuaBaoTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>
                                                       <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="shuaBaoSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="shuaBaoSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- 火山视频 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swHouShan"/>
                                                        <text text="火山视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="houShanTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>
                                                       <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="houShanSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="houShanSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>            
                                                    <!-- 火火视频 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swHouHou"/>
                                                        <text text="火火视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="houHouTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>
                                                       <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="houHouSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="houHouSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- 百度视频 -->
                                                    <horizontal w="*">
                                                        <Switch tag="Switch" id="swBaiDu"/>
                                                        <text text="百度视频" textColor="#000000" textStyle="bold"
                                                            width="70"/>
                                                        <input tag="EditText" id="baiDuTime" inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>
                                                       <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="baiDuSwitchAccountBegin" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number" id="baiDuSwitchAccountEnd" w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- 抖音视频 -->
                                                    <horizontal w="*">
                                                        <Switch />
                                                        <text text="抖音视频" textColor="#CDC5BF" textStyle="bold"
                                                            width="70"/>
                                                        <input inputType="number" text="60"
                                                            w="80" gravity="center"/>
                                                        <text text="分钟" textSize="16sp" w="auto" laygravity_center="right" w="60"/>

                                                        <text text="换号区间: " textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number"  w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="到" textStyle="bold" textSize="16sp"/>
                                                        <input tag="EditText" inputType="number"  w="80" text="0"
                                                            gravity="center"/>
                                                        <text text="之间" textStyle="bold" textSize="16sp"/>
                                                    </horizontal>
                                                    <!-- </list> -->
                                            </vertical>
                                        </scroll>
                                        <!-- 新闻阅读类 -->
                                        <vertical>
                                            <text text="新闻阅读类" textSize="18p" textStyle="bold" layout_gravity="center_horizontal"
                                                w="auto"/>
                                            <list id="newsList">
                                                <horizontal>
                                                    <Switch tag="Switch" id="{{appSwitchBtn}}"/>
                                                    <text text="{{appName}}" textColor="{{appNameColor}}" textStyle="bold"
                                                        width="70"/>
                                                    <text text=" 单次: "/>
                                                    <input tag="EditText" inputType="number" w="30"/>
                                                    <text text=" 上限: "/>
                                                    <input tag="EditText" inputType="number" w="30"/>
                                                    <button text="假装这个按钮有用" w="*" layout_gravity="right"/>
                                                </horizontal>
                                            </list>
                                        </vertical>
                                    </viewpager>
                                </card>
                                <button h="5" w="*"/>
                            </vertical>
                            <scroll>
                                <vertical>
                                    <text textSize="50" layout_gravity="center_horizontal" text="使用须知" gravity="center"/>
                                    <text id="precautions" textSize="30" layout_gravity="center_horizontal" gravity="left" h="*"/>
                                </vertical>
                            </scroll>
                            <!-- 功能 -->
                            <!-- <scroll>
                                <vertical>
                                    <text text="pageThree"/>
                                </vertical>
                            </scroll> -->
                        </viewpager>
                    </vertical>
                    <!-- 应用脚 -->
                    <vertical h="50" w="*">
                        <card h="50" bg="#D3D3D3">
                            <horizontal>
                                <button id="runAllBtn" text="启动掘金" typeSize="18sp" style="Widget.AppCompat.Button.Colored"
                                    layout_weight="1"/>
                                <button id="exitBtn" text="退出掘金" typeSize="18sp" style="Widget.AppCompat.Button.Colored"
                                    layout_weight="1"/>
                                <button id="checkUpdateBtn" text="检查更新" typeSize="18sp" style="Widget.AppCompat.Button.Colored" layout_weight="1"/>
                            </horizontal>
                        </card>
                    </vertical>
                </linear>
            </drawer>
